{
  "dashboard": {
    "id": null,
    "title": "Document Processing Analytics Dashboard",
    "tags": ["document-processing", "analytics", "business-intelligence"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Document Upload Trends",
        "type": "timeseries",
        "targets": [
          {
            "expr": "increase(document_uploads_total[1h])",
            "legendFormat": "Uploads per hour",
            "refId": "A"
          },
          {
            "expr": "increase(document_processing_total[1h])",
            "legendFormat": "Processing per hour",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 15,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none"
              },
              "axisPlacement": "auto",
              "axisLabel": "Count",
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Document Processing Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(document_processing_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(document_processing_time_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none"
              },
              "axisPlacement": "auto",
              "axisLabel": "Processing Time (seconds)",
              "scaleDistribution": {
                "type": "linear"
              }
            },
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 30
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Documents Processed Today",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(document_processing_total[24h])",
            "legendFormat": "Processed Today",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "fixed",
              "fixedColor": "green"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Document Processing Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(document_processing_success_total[5m])) / sum(rate(document_processing_total[5m])) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 90
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Document Processing Errors",
        "type": "timeseries",
        "targets": [
          {
            "expr": "increase(document_processing_errors_total[1h])",
            "legendFormat": "Errors per hour",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "bars",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 80,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none"
              },
              "axisPlacement": "auto",
              "axisLabel": "Errors",
              "scaleDistribution": {
                "type": "linear"
              }
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 2
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Document Types Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(document_types_total) by (document_type)",
            "legendFormat": "{{document_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "pie",
          "justifyMode": "auto",
          "displayLabels": ["name"],
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 14
        }
      },
      {
        "id": 7,
        "title": "File Size Distribution",
        "type": "bargauge",
        "targets": [
          {
            "expr": "sum(document_file_sizes_total) by (size_bucket)",
            "legendFormat": "{{size_bucket}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "bytes"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "horizontal",
          "displayMode": "gradient"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 14
        }
      },
      {
        "id": 8,
        "title": "Search Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(document_search_response_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(document_search_response_time_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 0,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none"
              },
              "axisPlacement": "auto",
              "axisLabel": "Response Time (ms)",
              "scaleDistribution": {
                "type": "linear"
              }
            },
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 22
        }
      },
      {
        "id": 9,
        "title": "Search Query Volume",
        "type": "timeseries",
        "targets": [
          {
            "expr": "increase(document_search_queries_total[1h])",
            "legendFormat": "Searches per hour",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "fixed",
              "fixedColor": "orange"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 3,
              "fillOpacity": 20,
              "gradientMode": "opacity",
              "spanNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none"
              },
              "axisPlacement": "auto",
              "axisLabel": "Searches",
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 22
        }
      },
      {
        "id": 10,
        "title": "Document Processing Status",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(document_processing_status_total) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [
              {
                "options": {
                  "completed": {
                    "color": "green",
                    "index": 0
                  },
                  "processing": {
                    "color": "yellow",
                    "index": 1
                  },
                  "failed": {
                    "color": "red",
                    "index": 2
                  },
                  "pending": {
                    "color": "blue",
                    "index": 3
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "pie",
          "justifyMode": "auto",
          "displayLabels": ["name", "value"],
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 28
        }
      },
      {
        "id": 11,
        "title": "Top Search Terms",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum(document_search_terms_total) by (search_term))",
            "legendFormat": "{{search_term}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "search_term"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "Search Term"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Value"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "Search Count"
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 28
        }
      },
      {
        "id": 12,
        "title": "System Analytics Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "system_analytics_total_contacts",
            "legendFormat": "Total Contacts",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "fixed",
              "fixedColor": "purple"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "purple",
                  "value": null
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 36
        }
      },
      {
        "id": 13,
        "title": "Total Documents",
        "type": "stat",
        "targets": [
          {
            "expr": "document_uploads_total",
            "legendFormat": "Total Documents",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "fixed",
              "fixedColor": "blue"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "blue",
                  "value": null
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 36
        }
      },
      {
        "id": 14,
        "title": "Average Complexity Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(document_complexity_score)",
            "legendFormat": "Average Complexity",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.3
                },
                {
                  "color": "red",
                  "value": 0.7
                }
              ]
            },
            "unit": "short",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "auto",
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 36
        }
      },
      {
        "id": 15,
        "title": "Documents per Contact",
        "type": "stat",
        "targets": [
          {
            "expr": "document_uploads_total / system_analytics_total_contacts",
            "legendFormat": "Docs per Contact",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "fixed",
              "fixedColor": "orange"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "orange",
                  "value": null
                }
              ]
            },
            "unit": "short",
            "decimals": 2
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 36
        }
      }
    ],
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "selected": true,
            "text": "Prometheus",
            "value": "Prometheus"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Processing Failures",
          "datasource": "-- Grafana --",
          "enable": true,
          "iconColor": "rgba(255, 0, 0, 1)",
